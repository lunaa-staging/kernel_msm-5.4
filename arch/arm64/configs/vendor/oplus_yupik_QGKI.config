# Architectures
CONFIG_ARCH_YUPIK=y

# NFC
CONFIG_NFC_ST21_I2C=y
CONFIG_NFC_ST54_SPI=y
CONFIG_OPLUS_NFC=y

# WiFi
CONFIG_CNSS_QCA6490=n
CONFIG_CNSS_QCA6750=y

# Fingerprint
# CONFIG_OPLUS_FINGERPRINT_GOODIX_OPTICAL is not set
CONFIG_OPLUS_FINGERPRINT_JIIOV_OPTICAL=y

# Touchpanel
CONFIG_TOUCHPANEL_FOCAL=y
CONFIG_TOUCHPANEL_FOCAL_FT3518=y
CONFIG_TOUCHPANEL_FOCAL_FT3658U=y

# CPU_IDLE Governor (TEO)
CONFIG_CPU_IDLE_GOV_TEO=y

# EROFS
CONFIG_EROFS_FS=y
# CONFIG_EROFS_FS_DEBUG is not set
CONFIG_EROFS_FS_XATTR=y
CONFIG_EROFS_FS_POSIX_ACL=y
CONFIG_EROFS_FS_SECURITY=y
CONFIG_EROFS_FS_ZIP=y
CONFIG_EROFS_FS_PCPU_KTHREAD=y
CONFIG_EROFS_FS_PCPU_KTHREAD_HIPRI=y
